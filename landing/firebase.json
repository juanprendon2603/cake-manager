{
  "hosting": [
    {
      "target": "landing",
      "public": "dist",
      "ignore": ["**/.*", "**/node_modules/**"],
      "rewrites": [{ "source": "**", "destination": "/index.html" }],
      "headers": [
        {
          "source": "**/*.@(js|css|png|jpg|jpeg|svg|webp|mp4)",
          "headers": [
            { "key": "Cache-Control", "value": "public,max-age=31536000" }
          ]
        }
      ]
    }
  ]
}
